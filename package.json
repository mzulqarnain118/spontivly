{
  "name": "spontivly",
  "version": "0.1.2",
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.15",
    "@mui/material": "^5.14.15",
    "@mui/styles": "^5.14.15",
    "@mui/x-tree-view": "^6.17.0",
    "@reduxjs/toolkit": "^1.9.7",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.6.0",
    "jodit-react": "^1.3.39",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-drag-drop-files": "^2.3.10",
    "react-hook-form": "^7.48.2",
    "react-multi-email": "^1.0.19",
    "react-query": "^3.39.3",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.17.0",
    "react-scripts": "5.0.1",
    "react-toastify": "^9.1.3",
    "reactjs-social-login": "^2.6.3",
    "redux": "^4.2.1",
    "redux-thunk": "^2.4.2",
    "styled-components": "^6.1.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "PORT=3082 env-cmd -f .env react-scripts start",
    "start:vite": "vite",
    "dev": "PORT=3082 env-cmd -f .env.local react-scripts start",
    "stg": "PORT=3082 env-cmd -f .env.staging react-scripts start",
    "build": "tsc && vite build",
    "build:no-ts": "vite build",
    "build:stg": "env-cmd -f .env.staging vite build",
    "build:prod": "env-cmd -f .env react-scripts build",
    "build:dev": "env-cmd -f .env.local react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "preview": "vite preview",
    "prepare": "husky install",
    "create-hook": "husky add .husky/pre-commit \"npm run lint\"",
    "lint": "eslint ."
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.23.3",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^29.5.10",
    "@types/node": "^20.10.3",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@vitejs/plugin-react": "^4.2.1",
    "env-cmd": "^10.1.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^40.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-n": "^16.3.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^6.2.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.0",
    "prettier": "3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "typescript": "^5.3.3",
    "vite": "^5.0.5",
    "vite-tsconfig-paths": "^4.2.1"
  }
}